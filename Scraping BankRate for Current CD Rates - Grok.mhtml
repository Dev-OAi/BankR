            if (result.candidates &amp;&amp; result.candidates.length &gt; =
0 &amp;&amp;
                result.candidates[0].content &amp;&amp; result.candidates[0=
].content.parts &amp;&amp;
                result.candidates[0].content.parts.length &gt; 0) {

