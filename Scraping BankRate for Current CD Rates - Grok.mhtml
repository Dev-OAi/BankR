                                        apy: { "type": "STRING" },
                                        minDeposit: { "type": "STRING" },
                                        term: { "type": "STRING" },
                                        validAsOf: { "type": "STRING" }
                                    },
                                    // Specify the order of properties to e=
nsure consistency
                                    propertyOrdering: ["bankName", "apy", "=
minDeposit", "term", "validAsOf"]
                                }
                            }
                        },
